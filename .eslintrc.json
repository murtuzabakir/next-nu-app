{
  "extends": ["next/core-web-vitals"],
  "ignorePatterns": ["out"],
  "rules": {
   "react/no-unescaped-entities": "warn",
    "no-console": "warn",
    "no-unused-vars": "off",
    "@typescript-eslint/no-unused-vars": [
      "warn",
      {
        "argsIgnorePattern": "^_",
        "varsIgnorePattern": "^_",
        "caughtErrorsIgnorePattern": "^_"
      }
    ]
  },
  "plugins": ["@typescript-eslint"]
}
